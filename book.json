{
  "plugins": [ "theme-api",  "asciidoc-include", "asciidoc-admonition-icons", "versions", "edit-link"],
  "pluginsConfig": {
    "theme-api": {
        "languages": [
          {
            "lang": "java",
            "name": "JavaScript",
            "default": true
          },
          {
            "lang": "xml",
            "name": "XML"
          },
          {
            "lang": "yml",
            "name": "YAML"
          }
         ]
    },
    "versions": {
        "type": "branches"
    },
    "edit-link": {
        "base": "https://github.com/rceuls/doc-webcmsmodule/edit/master/",
        "label": "Edit This Page"
    }
  }
}
