{
  "variables": {
        "moduleVersion": "0.0.3.RELEASE",
        "moduleUrl": "https://foreach.atlassian.net/wiki/display/AX/WebCmsModule",
        "javadoc": "http://across.foreach.be/docs/across-standard-modules/WebCmsModule/0.0.3.RELEASE/javadoc"
  },
  "plugins": [ "theme-api",  "asciidoc-include", "asciidoc-admonition-icons", "versions", "edit-link" ],
  "pluginsConfig": {
    "versions": {
      "type": "branches"
    },
    "edit-link": {
      "base": "https://github.com/ForeachOS/ax-docs-web-cms-module/edit/master/",
      "label": "Edit This Page"
    },
    "theme-api": {     
        "languages": [
          {
            "lang": "java",
            "name": "JavaScript",
            "default": true
          },
          {
            "lang": "xml",
            "name": "XML"
          },
          {
            "lang": "yml",
            "name": "YAML"
          }
         ]
    }
  }
}

